{
  "expo": {
    "name": "UseYourTools",
    "slug": "useyourtools",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "backgroundColor": "#f5f5dc",
      "resizeMode": "contain",
      "image": "./assets/icon.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kmarsten.useyourtools",
      "config": {
        "usesNonExemptEncryption": false
      },
      "newArchEnabled": false,
      "infoPlist": {
        "NSCalendarsUsageDescription": "This app needs access to your calendar to sync events.",
        "NSCalendarsFullAccessUsageDescription": "This app needs full access to your calendar to sync events.",
        "NSRemindersUsageDescription": "This app needs access to your reminders to sync events.",
        "NSRemindersFullAccessUsageDescription": "This app needs full access to your reminders to sync events."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#f5f5dc"
      },
      "newArchEnabled": false,
      "package": "com.useyourtools.app",
      "permissions": [
        "READ_CALENDAR",
        "WRITE_CALENDAR",
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ]
    },
    "web": {},
    "scheme": "useyourtools",
    "plugins": [
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "43ad00a3-7e94-43eb-9462-340003fc7e70"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/43ad00a3-7e94-43eb-9462-340003fc7e70",
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0
    }
  }
}
